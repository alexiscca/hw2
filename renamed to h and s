<!DOCTYPE html>
<html>
<head>
<script src="https://cdnjs.cloudflare.com/ajax/libs/p5.js/0.7.1/p5.min.js"></script>
<style>
  html, body {
    margin: 0;
    padding: 0;
  }
</style>
<meta charset="utf-8" />
</head>
<body>
<script>

function setup() {
    createCanvas(400, 400);
    background(220);
    colorMode(400, 400, 100);
}

function draw() {
  let h = random(width);
  let s = random(height);  
  fill(h, s, 100);
    noStroke()
    ellipse(h, s, 20);
}


</script>
</body>
</html>
